<%- include('../../partials/header.ejs') %>
<%- include('../../partials/navbar.ejs') %>
<body>
    <div class="container">
        <div class="card mt-5 ">
            <div class="card-header">
                <h2 class="text-center"><%= article.title %></h2>
            </div>
            <div class="card-body">
                <%- article.body %>
            </div>
            <div class="row">
                <div class="col text-center" style="color:#007BFF">
                    <a href="/like/<%= article.id %>/<%= article.slug %>"><i class="fas fa-thumbs-up  m-4"></i><%= article.like %></a>
                    <a href="/deslike/<%= article.id %>/<%= article.slug %>"><i class="fas fa-thumbs-down  m-4"></i><%= article.deslike %></a>
                </div>
            </div>
        </div>
    </div>
</body>
<%- include('../../partials/footer.ejs') %>